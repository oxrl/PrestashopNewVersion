rar que respetas las obligaciones que estipula el reglamento.","tab":"","version":"1.0.1","compatibility":{"from":"1.7.0.0","to":"1.7.3.2"},"nbRates":"0","product_type":"module","avgRate":"0.0","price":{"EUR":0,"USD":0,"GBP":0},"badges":[{"label":"Made by PrestaShop","img":"https:\/\/medias2.prestastore.com\/themes\/prestastore\/img\/front\/sprites\/badges\/made-by-prestashop.png"}],"cover":{"small":"https:\/\/api-addons.prestashop.com\/1034861-pprod\/official-gdpr-compliance-by-prestashop-17.jpg","big":"https:\/\/api-addons.prestashop.com\/1034861-pbig\/official-gdpr-compliance-by-prestashop-17.jpg"}}]}";